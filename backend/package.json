{
  "name": "pnw-alliance-manager-backend",
  "version": "1.0.0",
  "description": "Backend API for Politics and War Alliance Manager",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc && npx prisma generate",
    "start": "npx prisma migrate deploy && node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx src/config/seed.ts"
  },
  "keywords": [
    "politics-and-war",
    "alliance-manager",
    "discord-oauth",
    "graphql",
    "api"
  ],
  "author": "OrbixTech",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-jwt": "^4.0.1",
    "winston": "^3.10.0",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.3",
    "@types/passport": "^1.0.13",
    "@types/passport-discord": "^0.1.8",
    "@types/passport-jwt": "^3.0.9",
    "@types/node-cron": "^3.0.8",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "ts-jest": "^29.1.1",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}